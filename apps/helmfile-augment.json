{
  "releases": [
    {
      "chart": "jxlabs-nos-helmboot-resources/jxlabs-nos-helmboot-resources",
      "hooks": [
        {
          "args": [
            "step",
            "scheduler",
            "config",
            "apply",
            "--direct=true"
          ],
          "command": "jx",
          "events": [
            "postsync"
          ],
          "showlogs": true
        }
      ],
      "needs": [
        "jx/jenkins-x-crds"
      ]
    },
    {
      "chart": "jenkins-x/tekton",
      "needs": [
        "jx/jxlabs-nos-helmboot-resources"
      ]
    },
    {
      "chart": "jenkins-x/prow",
      "needs": [
        "jx/tekton"
      ]
    },
    {
      "chart": "jenkins-x/jxui",
      "needs": [
        "jx/jenkins-x-crds"
      ]
    },
    {
      "chart": "../jenkins-servers",
      "needs": [
        "jx/jenkins-operator"
      ]
    },
    {
      "chart": "../repositories",
      "needs": [
        "jx/jenkins-x-crds"
      ]
    }
  ]
}
